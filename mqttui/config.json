{
  "arch": [
    "amd64",
    "aarch64"
  ],
  "boot": "auto",
  "description": "mqttui的网络图形用户界面（Web GUI）。\n\n请注意，\"Web GUI\" 是指网络图形用户界面，而 \"mqttui\" 是一个开源的 MQTT 客户端应用程序。",
  "environment": {},
  "map": [
    "share:rw"
  ],
  "version": "v1.3.0",
  "name": "mqttui",
  "options": {
    "MQTT_BROKER": "hassio.local",
    "MQTT_PORT": 1883,
    "MQTT_USERNAME": "hassio",
    "MQTT_PASSWORD": "password"
  },
  "ports": {
    "5000/tcp": "5000"
  },
  "ports_description": {
    "5000/tcp": "Web interface"
  },
  "schema": {
    "MQTT_BROKER": "str",
    "MQTT_PORT": "int",
    "MQTT_USERNAME": "str",
    "MQTT_PASSWORD": "str"
  },
  "slug": "mqttui",
  "startup": "services",
  "upstream": "v1.3.0",
  "url": "https://gh-proxy.com/github.com/jdeath/homeassistant-addons",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:5000]",
  "ingress": false,
  "ingress_port": 5000
}
