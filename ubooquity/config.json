  "image": "ghcr.nju.edu.cn/alexbelgium/ubooquity-{arch}",
  "init": false,
  "map": [
    "media:rw",
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "name": "Ubooquity",
  "options": {
    "PGID": 0,
    "PUID": 0,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "maxmem": 200,
    "ssl": false,
    "theme": "default"
  },
  "ports": {
    "2205/tcp": 2202,
    "2206/tcp": 2203
  },
  "ports_description": {
    "2205/tcp": "Library page (to expose)",
    "2206/tcp": "Admin page (local)"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "PGID": "int",
    "PUID": "int",
    "TZ": "str?",
    "certfile": "str",
    "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "keyfile": "str",
    "localdisks": "str?",
    "maxmem": "int",
    "networkdisks": "str?",
    "smbv1": "bool?",
    "ssl": "bool",
    "theme": "list(default|comixology2|plextheme-master)"
  },
  "slug": "ubooquity",
  "udev": true,
  "url": "https://gh-proxy.com/github.com/alexbelgium/hassio-addons",
  "version": "2.1.5",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:2206]/ubooquity/admin"
