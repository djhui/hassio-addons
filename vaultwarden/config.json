  "environment": {
    "DATA_FOLDER": "/config"
  },
  "map": [
    "addon_config:rw"
  ],
  "image": "ghcr.nju.edu.cn/dani-garcia/vaultwarden",
  "version": "1.33.2",
  "name": "Vaultwarden",
  "options": {},
  "ports": {
    "80/tcp": "7277"
  },
  "ports_description": {
    "80/tcp": "Web interface"
  },
  "schema": {},
  "slug": "vaultwarden",
  "startup": "services",
  "upstream": "1.33.2",
  "url": "https://gh-proxy.com/github.com/jdeath/homeassistant-addons",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "ingress": true,
  "ingress_port": 80
}
