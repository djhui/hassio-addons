{
  "arch": [
    "amd64",
    "aarch64"
  ],
  "boot": "auto",
  "description": "Inventory and organization system built for the Home User",
  "environment": {
    "HBOX_STORAGE_DATA": "/config",
    "HBOX_STORAGE_SQLITE_URL": "/config/homebox.db?_fk=1"
  },
  "map": [
    "addon_config:rw"
  ],
  "version": "0.18.0",
  "name": "Homebox",
  "options": {
    "HBOX_OPTIONS_ALLOW_REGISTRATION": "true"
  },
  "ports": {
    "7745/tcp": "7745"
  },
  "ports_description": {
    "7745/tcp": "Web interface"
  },
  "schema": {
    "HBOX_OPTIONS_ALLOW_REGISTRATION": "list(true|false)"
  },
  "slug": "homebox",
  "startup": "services",
  "upstream": "0.18.0",
  "url": "https://gh-proxy.com/github.com/jdeath/homeassistant-addons",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:7745]",
  "ingress": false,
  "ingress_port": 7745
}
