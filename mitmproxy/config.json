{
  "name": "mitmproxy",
  "version": "1.2.0",
  "slug": "mitmproxy",
  "panel_icon": "mdi:lan-connect",
  "description": "(折旧) 一个免费的开源交互式HTTPS代理程序，用于拦截和检查网络流量。\n\n翻译：一个免费的、开源的、交互式的HTTPS代理程序，可以用来拦截和检查网络流量。",
  "url": "https://gh-proxy.com/github.com/Poeschl-HomeAssistant-Addons/mitmproxy",
  "arch": [
    "armv7",
    "armhf",
    "aarch64",
    "amd64",
    "i386"
  ],
  "hassio_api": true,
  "image": "ghcr.nju.edu.cn/poeschl-homeassistant-addons/mitmproxy-{arch}",
  "map": [
    "ssl",
    "share:rw"
  ],
  "ports": {
    "8081/tcp": 8081,
    "8080/tcp": 8080
  },
  "ports_description": {
    "8081/tcp": "Web Frontend",
    "8080/tcp": "Proxy listening port"
  },
  "options": {
    "options": [
      {
        "name": "anticache",
        "value": "true"
      }
    ]
  },
  "schema": {
    "options": [
      {
        "name": "str",
        "value": "str"
      }
    ],
    "custom_ca": "match(^/ssl/.*)?"
  }
}
