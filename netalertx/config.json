{
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "🖧🔍 WIFI/LAN扫描器、入侵者和存在探测器\n\n这个短语描述了一个工具或设备，它能够扫描无线网络（WIFI）和有线局域网（LAN），检测潜在的入侵者，并探测是否存在未授权的连接。",
  "environment": {
    "PGID": "102",
    "PORT": "20211",
    "PUID": "102",
    "TZ": "Europe/Berlin"
  },
  "hassio_api": true,
  "host_network": true,
  "image": "ghcr.nju.edu.cn/alexbelgium/netalertx-{arch}",
  "ingress": true,
  "ingress_port": 0,
  "ingress_stream": true,
  "init": false,
  "map": [
    "addon_config:rw",
    "media:rw",
    "share:rw",
    "ssl"
  ],
  "name": "NetAlertX",
  "panel_icon": "mdi:wifi-check",
  "ports": {
    "20211/tcp": 20211,
    "20212/tcp": 20212
  },
  "ports_description": {
    "20211/tcp": "WebUI port",
    "20212/tcp": "GraphQL port"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH",
    "NET_ADMIN",
    "NET_RAW"
  ],
  "schema": {
    "APP_CONF_OVERRIDE": "str?",
    "TZ": "str?"
  },
  "services": [
    "mqtt:want"
  ],
  "slug": "netalertx",
  "udev": true,
  "url": "https://gh-proxy.com/github.com/alexbelgium/hassio-addons",
  "version": "25.3.1"
}
