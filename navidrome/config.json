{
  "arch": [
    "amd64",
    "aarch64"
  ],
  "backup_exclude": [
    "**/cache/**"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "Navidrome for Home Assistant 的意思是“为家庭助手（Home Assistant）的 Navidrome”。Navidrome 是一个开源的音乐流媒体服务器，而 Home Assistant 是一个智能家居自动化平台。将 Navidrome 集成到 Home Assistant 中，可以让用户通过他们的智能家居系统来访问和管理音乐库。",
  "image": "ghcr.nju.edu.cn/alexbelgium/navidrome-{arch}",
  "init": false,
  "map": [
    "addon_config:rw",
    "media",
    "share:rw",
    "ssl:rw"
  ],
  "name": "Navidrome",
  "options": {
    "base_url": "/",
    "certfile": "fullchain.pem",
    "data_folder": "/data",
    "keyfile": "privkey.pem",
    "log_level": "info",
    "music_folder": "/data/music",
    "ssl": false
  },
  "ports": {
    "4533/tcp": 4533
  },
  "ports_description": {
    "4533/tcp": "Web interface"
  },
  "schema": {
    "base_url": "str",
    "certfile": "str?",
    "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "data_folder": "str",
    "default_language": "str?",
    "image_cache_size": "str?",
    "keyfile": "str?",
    "lastfm_api_key": "str?",
    "lastfm_secret": "str?",
    "localdisks": "str?",
    "log_level": "str",
    "music_folder": "str",
    "networkdisks": "str?",
    "password_encryption_key": "str?",
    "scan_schedule": "str?",
    "spotify_id": "str?",
    "spotify_secret": "str?",
    "ssl": "bool",
    "transcoding_cache_size": "str?",
    "welcome_message": "str?"
  },
  "slug": "navidrome",
  "udev": true,
  "url": "https://gh-proxy.com/github.com/alexbelgium/hassio-addons/tree/master/navidrome",
  "version": "0.54.5",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:4533]"
}
