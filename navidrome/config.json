{
  "arch": [
    "amd64",
    "aarch64"
  ],
  "backup_exclude": [
    "**/cache/**"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "纳维德罗姆（Navidrome）是一款开源的音乐流媒体服务器，它允许用户在本地网络中托管自己的音乐库，并通过各种客户端应用程序进行访问和播放。纳维德罗姆通常与家庭自动化系统如Home Assistant集成，以便通过语音控制或其他智能家居设备来管理音乐播放。\n\n纳维德罗姆支持多种音频格式，并提供标签编辑、专辑艺术管理等高级功能。它可以通过Web界面或命令行工具进行配置和管理，并且兼容多种操作系统，包括Linux、Windows和macOS。\n\n将纳维德罗姆集成到Home Assistant中，可以让用户通过Alexa、Google Assistant等智能音箱或智能家居控制器来控制音乐的播放和切换，实现更加便捷的音乐体验。",
  "image": "ghcr.nju.edu.cn/alexbelgium/navidrome-{arch}",
  "init": false,
  "map": [
    "addon_config:rw",
    "media",
    "share:rw",
    "ssl:rw"
  ],
  "name": "Navidrome",
  "options": {
    "base_url": "/",
    "certfile": "fullchain.pem",
    "data_folder": "/data",
    "keyfile": "privkey.pem",
    "log_level": "info",
    "music_folder": "/data/music",
    "ssl": false
  },
  "ports": {
    "4533/tcp": 4533
  },
  "ports_description": {
    "4533/tcp": "Web interface"
  },
  "schema": {
    "base_url": "str",
    "certfile": "str?",
    "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "data_folder": "str",
    "default_language": "str?",
    "image_cache_size": "str?",
    "keyfile": "str?",
    "lastfm_api_key": "str?",
    "lastfm_secret": "str?",
    "localdisks": "str?",
    "log_level": "str",
    "music_folder": "str",
    "networkdisks": "str?",
    "password_encryption_key": "str?",
    "scan_schedule": "str?",
    "spotify_id": "str?",
    "spotify_secret": "str?",
    "ssl": "bool",
    "transcoding_cache_size": "str?",
    "welcome_message": "str?"
  },
  "slug": "navidrome",
  "udev": true,
  "url": "https://gh-proxy.com/github.com/alexbelgium/hassio-addons/tree/master/navidrome",
  "version": "0.54.5",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:4533]"
}
